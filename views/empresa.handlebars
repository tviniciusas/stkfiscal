<div class="row">
    <div class="col-lg-12">
        <div class="card card-primary">
        <div class="card-header">
            <h3 class="card-title">Dados cadastrais</h3>
        </div>
        <!-- /.card-header -->
        <!-- form start -->
        <form role="form">
        <div class="card-body">
            <div class="row">
                <div class="col-sm-8">
                    <div class="form-group">
                        <label>Nome</label>
                        <input type="text" class="form-control" placeholder="Informe o nome da sua empresa">
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label>Inscrição Estadual</label>
                        <input type="text" class="form-control" placeholder="Informe a inscrição estadual da sua empresa">
                    </div>
                </div>
                <div class="col-sm-8">
                    <div class="form-group">
                        <label>Razão Social</label>
                        <input type="text" disabled="disabled" value="{{empresa.razao}}" class="form-control" placeholder="Informe a razão social da sua empresa">
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label>CNPJ</label>
                        <input type="text" disabled="disabled" class="form-control" value="{{empresa.cnpj}}" placeholder="Informe o CNPJ da sua empresa">
                    </div>
                </div>
                <div class="col-sm-8">
                    <div class="form-group">
                        <label for="exampleInputEmail1">E-mail comercial</label>
                        <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Informe o e-mail comercial da empresa">
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label>Telefone</label>
                        <input type="text" class="form-control" placeholder="Informe o telefone comercial">
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                    <label>Estado</label>
                    <select class="form-control select2bs4" style="width: 100%;">
                        <option selected="selected">Alabama</option>
                        <option>Alaska</option>
                        <option>California</option>
                        <option>Delaware</option>
                        <option>Tennessee</option>
                        <option>Texas</option>
                        <option>Washington</option>
                    </select>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                    <label>Cidade</label>
                    <select class="form-control select2bs4" disabled="disabled" style="width: 100%;">
                        <option selected="selected">Alabama</option>
                        <option>Alaska</option>
                        <option>California</option>
                        <option>Delaware</option>
                        <option>Tennessee</option>
                        <option>Texas</option>
                        <option>Washington</option>
                    </select>
                    </div>
                </div>
            </div>

            <div class="socio-adm mt-4">
                <h5>Sócio Administrador</h5>
                <hr class="solid mt-0 mb-3">
                <div class="row">
                    <div class="col-sm-8">
                        <div class="form-group">
                            <label>Nome</label>
                            <input type="text" class="form-control" placeholder="Informe o nome completo do sócio administrador">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label>Telefone</label>
                            <input type="text" class="form-control" placeholder="Informe o telefone do sócio administrador">
                        </div>
                    </div>
                </div>
            </div>

            
            <div class="fiscal-interno mt-4">
                <h5>Fiscal Interno</h5>
                <hr class="solid mt-0 mb-3">
                <div class="row">
                    <div class="col-sm-8">
                        <div class="form-group">
                            <label>Nome</label>
                            <input type="text" class="form-control" placeholder="Informe o nome completo do fiscal interno">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label>Telefone</label>
                            <input type="text" class="form-control" placeholder="Informe o telefone do fiscal interno">
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="form-group">
                                <label for="exampleInputEmail1">E-mail</label>
                                <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Informe o e-mail do fiscal interno">
                        </div>
                    </div>
                </div>
            </div>

            <div class="sefaz mt-4">
                <h5>SEFAZ</h5>
                <hr class="solid mt-0 mb-3">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label>Usuário SEFAZ</label>
                            <input type="text" class="form-control" placeholder="Usuário utilizado na SEFAZ">
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label>Senha SEFAZ</label>
                            <input type="text" class="form-control" placeholder="Senha utilizada na SEFAZ">
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <!-- /.card-body -->

        <div class="card-footer">
            <button type="submit" class="btn btn-primary">Salvar</button>
        </div>
        </form>
        </div>
    </div>
</div>
